<h2>{{title}}</h2>
<table>
    <thead>
        <tr>
            <th>Movie ID</th>
            <th>Title</th>
            <th>Year</th>
            <th>Genre</th>
            <th>Director</th>
        </tr>
    </thead>
    <tbody>
        {{#each sales}}
            <tr>
                <td>{{this.Movie_ID}}</td>
                <td>{{this.Title}}</td>
                <td>{{this.Year}}</td>
                <td>{{this.Genre}}</td>
                <td>{{this.Director}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
